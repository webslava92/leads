  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "tsc:watch": "tsc -w",
    "server:dev": "concurrently -k \"tsc -w\" \"nodemon --inspect dist/server/index.js\"",
    "electron:serve": "concurrently -k \"cross-env BROWSER=none yarn start\" \"yarn electron:start\"",
    "electron:build": "yarn build && electron-builder -c.extraMetadata.main=build/electron.js",
    "electron:start": "wait-on tcp:127.0.0.1:3000 && electron ."
  },
  "browserslist": {
    "production": [
      "last 1 electron version",
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 electron version",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "husky": {},
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "eslint . --ext ts --ext tsx --fix"
    ]
  }
